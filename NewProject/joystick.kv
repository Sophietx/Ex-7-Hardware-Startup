<JoystickScreen>:
    #: import time __main__.time

    name: 'joystickscreen'
    FloatLayout:
        size_hint: None, None

    DPEAButton:
        id: test7_button
        text: 'go to mainscreen'
        size: 100, 100
        x: root.width * 0.5
        y: root.height * 0.25
        color: 0.5, 1, 0.7, 1
        on_press: root.transitionback(test7_button)

    DPEAButton:
        id: joystick_button
        text: ''
        size: 100, 100
        x: root.width * 0.5
        y: root.height * 0.75
        color: 1, 0.7, 0.7, 1

    DPEAButton:
        id: joystick_buttonss
        text: ''
        size: 100, 100
        x: root.width * 0.25
        y: root.height * 0.5
        color: 0.3, 0.5, 0.7, 1
        